{
  "expo": {
    "name": "SureTopUp",
    "slug": "suretopup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "suretopup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "dark": {
        "image": "./assets/images/splash-icon.png"
      },
      "imageWidth": 200
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suretopup.app",
      "infoPlist": {
        "STUPhoneNumberUsageDescription": "Phone number is collected to identify the specific account receiving airtime and data, and to verify the user for secure transactions.",
        "STUStateUsageDescription": "State information is collected to determine regional service availability and comply with local KYC and tax regulations for financial transactions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.suretopup.app",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow SureTopUp to save receipt images to your photo library.",
          "savePhotosPermission": "Allow SureTopUp to save receipt images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56978455-463b-4c14-bb34-ea40d2168f7b"
      }
    }
  }
}
